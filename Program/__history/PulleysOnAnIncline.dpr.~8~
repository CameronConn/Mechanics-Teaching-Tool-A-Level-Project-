program PulleysOnAnIncline;



{$R *.dres}

uses
  FMX.Forms,
  MainMenuUnit in 'MainMenuUnit.pas' {MainMenuForm},
  OwnModelUnit in 'OwnModelUnit.pas' {OwnModelForm},
  PracticeUnit in 'PracticeUnit.pas' {PracticeForm},
  BallUnit in 'BallUnit.pas' {BallForm};

{$R *.res}

begin
  Application.Initialize;
  Application.CreateForm(TMainMenuForm, MainMenuForm);
  Application.CreateForm(TOwnModelForm, OwnModelForm);
  Application.CreateForm(TPracticeForm, PracticeForm);
  Application.CreateForm(TBallForm, BallForm);
  Application.Run;
end.
